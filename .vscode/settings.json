{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.venv": true,
        "yuuno2.js/**/lib": true,
        "yuuno2.js/**/dist": true
    },
    "python.pythonPath": "python/.venv/bin/python3.8",
    "python.testing.unittestEnabled": false,
    "python.testing.pytestArgs": [
        "python",
    ],
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true
}